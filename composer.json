{
    "name": "zubayer/corporate-website",
    "description": "A brief description of your awesome PHP project.",
    "keywords": ["php", "framework", "example"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "some-vendor/some-package": "^1.0",
        "another-vendor/another-package": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "App\\": "apps/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs"
    },
    "config": {
        "sort-packages": true
